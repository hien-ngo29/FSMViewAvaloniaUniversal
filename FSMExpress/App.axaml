<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="FSMExpress.App"
             xmlns:local="using:FSMExpress"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
      <SimpleTheme />
      <StyleInclude Source="/Theming/SolidMenuItemStyle.axaml" />
      <StyleInclude Source="/Theming/CodeColors.axaml" />
      <StyleInclude Source="/Theming/ListColors.axaml" />
      <StyleInclude Source="/Theming/TabStyle.axaml" />
    </Application.Styles>
</Application>